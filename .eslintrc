{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "rules": {
      "func-names": 0,
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "arrow-body-style": 0,
      "import/prefer-default-export": 0,
      new-cap: 0
    },
    "globals": {
        "ReactNative": true,
        "React": true,
        "Immutable": true,
    },
    "env": {
      "node": true,
      "jest": true
    }
}
